  "repository": {
    "type": "git",
    "url": "git+https://github.com/ianhomer/pattern-accessibility.git"
  },
  "bugs": {
    "url": "https://github.com/ianhomer/pattern-accessibility/issues"
  },
  "homepage": "https://github.com/ianhomer/pattern-accessibility#readme",
  "main": "index.js",
  "scripts": {
    "build": "parcel build src/index.html",
    "clean": "rimraf dist",
    "dev": "parcel src/index.html --lazy",
