    "prettier:fix": "npx prettier --write .",
    "start": "python3 -m http.server -d dist"
  },
  "devDependencies": {
    "format-package": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "parcel": "^2.4.1",
    "prettier": "^2.6.2"
